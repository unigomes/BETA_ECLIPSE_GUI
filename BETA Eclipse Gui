local function PlayAnim(id)
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["Character%2"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%5"]['LocalPlayer']['Character'];
	if not TABLE_TableIndirection["TABLE_TableIndirection%0"]["Character%2"] then
		return;
	end
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["Humanoid%1"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Character%2"]:FindFirstChildOfClass("Humanoid");
	if not TABLE_TableIndirection["TABLE_TableIndirection%0"]["Humanoid%1"] then
		return;
	end
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["Anim%0"] = Instance.new("Animation");
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["Anim%0"]['AnimationId'] = "rbxassetid://" .. id;
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["animationTrack%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Humanoid%1"]:LoadAnimation(TABLE_TableIndirection["TABLE_TableIndirection%0"]["Anim%0"]);
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["animationTrack%0"]:Play();
end
