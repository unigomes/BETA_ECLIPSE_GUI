local function castSpell(spell)
	if not TABLE_TableIndirection["TABLE_TableIndirection%0"]["KeybindsEnabled%0"] then
		return;
	end
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["target%0"] = (game['Players']['LocalPlayer']:GetMouse()['Target'] and game['Players']['LocalPlayer']:GetMouse()['Target']['Parent']) or nil;
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["args%0"] = {[1]={Incant=spell['Command'],Target=((spell['Mass'] and nil) or TABLE_TableIndirection["TABLE_TableIndirection%0"]["target%0"])}};
	game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("WitchSpell"):FireServer(unpack(TABLE_TableIndirection["TABLE_TableIndirection%0"]["args%0"]));
end
local function ToggleKeybinds(state)
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["KeybindsEnabled%0"] = state;
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["ActiveKeybinds%0"] = {};
	if state then
		for _, category in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["SpellCategories%0"]) do
			for _, spell in ipairs(category.Spells) do
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["key%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["NumberKeyMap%0"][spell['DefaultKey']] or spell['DefaultKey']:upper();
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["ActiveKeybinds%0"][TABLE_TableIndirection["TABLE_TableIndirection%0"]["key%0"]] = function()
					castSpell(spell);
				end;
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["DefaultKeybinds%0"][spell['Name']] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["key%0"];
			end
		end
	end
end
TABLE_TableIndirection["TABLE_TableIndirection%0"]["KeybindsTab%0"]:Toggle({Title="Enable All Keybinds & General Spells Defaults",Desc="This activates all keybinds but only general spells defaults, Gamepasess spells must be put manually",Default=false,Callback=function(state)
	ToggleKeybinds(state);
end});
