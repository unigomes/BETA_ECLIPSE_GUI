		TABLE_TableIndirection["TABLE_TableIndirection%0"]["depthOfField%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Lighting%0"]:FindFirstChildOfClass("DepthOfFieldEffect");
