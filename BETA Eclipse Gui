				repeat
					TABLE_TableIndirection["TABLE_TableIndirection%0"]["MoonVial%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Player%2"]['Backpack']:FindFirstChild("Moon Vial");
					wait(0);
				until TABLE_TableIndirection["TABLE_TableIndirection%0"]["MoonVial%0"] or not TABLE_TableIndirection["TABLE_TableIndirection%0"]["AutoMoonVialToggle%0"] 
				if not TABLE_TableIndirection["TABLE_TableIndirection%0"]["AutoMoonVialToggle%0"] then
					break;
				end
				if TABLE_TableIndirection["TABLE_TableIndirection%0"]["MoonVial%0"] then
					TABLE_TableIndirection["TABLE_TableIndirection%0"]["MoonVial%0"]['Parent'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Player%2"]['Character'];
					TABLE_TableIndirection["TABLE_TableIndirection%0"]["MoonVial%0"]:Activate();
					wait(0);
				end
			end
		end)();
	end
end});
