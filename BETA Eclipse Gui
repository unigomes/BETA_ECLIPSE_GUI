		spawn(function()
			while TABLE_TableIndirection["TABLE_TableIndirection%0"]["spamTargetActive%1"] do
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"] = nil;
				if TABLE_TableIndirection["TABLE_TableIndirection%0"]["spamModeIsCharacter%0"] then
					for _, p in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%10"]:GetPlayers()) do
						if p:FindFirstChild("CharacterConfiguration") then
							TABLE_TableIndirection["TABLE_TableIndirection%0"]["charName%0"] = p['CharacterConfiguration']['CharacterName']['Value']:lower();
							if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["charName%0"] ~= TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetInput%0"]) then
							else
								TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"] = p['Character'];
								break;
							end
						end
					end
				else
					for _, p in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%10"]:GetPlayers()) do
						if (p['Name']:lower() ~= TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetInput%0"]) then
						else
							TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"] = p['Character'];
							break;
						end
					end
				end
				if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"] and TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"]['PrimaryPart']) then
					for _, spell in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedTargetSpamSpells%0"]) do
						TABLE_TableIndirection["TABLE_TableIndirection%0"]["args%0"] = {[1]={Incant=spell,Target=TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"]}};
						TABLE_TableIndirection["TABLE_TableIndirection%0"]["ReplicatedStorage%8"]:WaitForChild("RemoteEvents"):WaitForChild("WitchSpell"):FireServer(unpack(TABLE_TableIndirection["TABLE_TableIndirection%0"]["args%0"]));
					end
				end
				wait(0);
			end
		end);
	end
end});
