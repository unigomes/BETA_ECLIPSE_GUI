		end
	end
	local function FindTargetCharacter(targetName)
		for _, player in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%4"]:GetPlayers()) do
			if player:FindFirstChild("CharacterConfiguration") then
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["charConfig%0"] = player['CharacterConfiguration'];
				if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["charConfig%0"]:FindFirstChild("CharacterName") and (TABLE_TableIndirection["TABLE_TableIndirection%0"]["charConfig%0"]['CharacterName']['Value']:lower() == targetName:lower())) then
					return player['Character'];
				end
			end
		end
		return nil;
	end
	local function CastSpySpell(targetCharacter)
		if targetCharacter then
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["animationTrack%0"] = PlayAnimation(TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%2"].Character);
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["originalCameraSubject%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Camera%1"]['CameraSubject'];
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["Camera%1"]['CameraSubject'] = targetCharacter:FindFirstChild("Humanoid");
			task.delay(TABLE_TableIndirection["TABLE_TableIndirection%0"]["SPELL_DURATION%0"], function()
				if TABLE_TableIndirection["TABLE_TableIndirection%0"]["animationTrack%0"] then
					TABLE_TableIndirection["TABLE_TableIndirection%0"]["animationTrack%0"]:Stop();
				end
				if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%2"]['Character'] and TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%2"]['Character']:FindFirstChild("Humanoid")) then
					TABLE_TableIndirection["TABLE_TableIndirection%0"]["Camera%1"]['CameraSubject'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%2"]['Character']:FindFirstChild("Humanoid");
				else
					TABLE_TableIndirection["TABLE_TableIndirection%0"]["Camera%1"]['CameraType'] = Enum['CameraType']['Custom'];
				end
			end);
		end
	end
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%2"]['Chatted']:Connect(function(message)
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["loweredMessage%0"] = message:lower():gsub(",", ""):match("^%s*(.-)%s*$");
		for _, spellName in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["SPELL_NAMES%0"]) do
			if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["loweredMessage%0"]:sub(1, #spellName) ~= spellName) then
			else
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetName%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["loweredMessage%0"]:sub(#spellName + 2):lower();
				for shortName, fullName in pairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["CHARACTER_NAME_MAP%0"]) do
					if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetName%0"]:sub(1, #shortName) ~= shortName:lower()) then
					else
						TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetName%0"] = fullName:lower();
						break;
					end
				end
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"] = FindTargetCharacter(TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetName%0"]);
				if TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"] then
					CastSpySpell(TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"]);
				else
					print("Character not found.");
				end
				return;
			end
		end
	end);
end
local function StopCustomSpells()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["CustomSpellsEnabled%0"] = false;
end
TABLE_TableIndirection["TABLE_TableIndirection%0"]["GrimoireTab%0"]:Toggle({Title="Enable Custom Spells",Default=false,Callback=function(state)
	if state then
		StartCustomSpells();
	else
		StopCustomSpells();
	end
end});
local function EnableSpellAutofill()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["Chat%0"] = game['Players']['LocalPlayer']['PlayerGui']:WaitForChild("Chat");
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["SpellsList%0"] = {"Phasmatos Eclipse Spectra Ducite Me ","Tenebras Magica Restituare"};
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["Frame%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Chat%0"]:WaitForChild("Frame");
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["ChatBar%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Frame%0"]:WaitForChild("ChatBarParentFrame"):WaitForChild("Frame"):WaitForChild("BoxFrame"):WaitForChild("Frame"):WaitForChild("ChatBar");
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["SuggestionFrame%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Frame%0"]:WaitForChild("SuggestionFrame");
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["ChatBar%0"]:GetPropertyChangedSignal("Text"):Connect(function()
		if not TABLE_TableIndirection["TABLE_TableIndirection%0"]["CustomSpellsEnabled%0"] then
			return;
		end
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["suggestions%0"] = {};
		for _, spell in pairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["SpellsList%0"]) do
			if (spell:lower():sub(1, #TABLE_TableIndirection["TABLE_TableIndirection%0"]["ChatBar%0"]['Text']) ~= TABLE_TableIndirection["TABLE_TableIndirection%0"]["ChatBar%0"]['Text']:lower()) then
			else
				table.insert(TABLE_TableIndirection["TABLE_TableIndirection%0"]["suggestions%0"], spell);
			end
		end
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["SuggestionFrame%0"]['Size'] = UDim2.new(1, 0, 0, 48);
		for _, suggestion in pairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["suggestions%0"]) do
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"] = Instance.new("TextButton");
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['Name'] = suggestion;
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['BackgroundColor3'] = Color3.new(0, 0, 0);
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['BackgroundTransparency'] = 0.6;
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['Size'] = UDim2.new(0, game['TextService']:GetTextSize(suggestion, 18, Enum['Font'].Fantasy, Vector2.new(10000, 10000))['X'] + 6, 0, 30);
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['TextColor3'] = Color3.new(1, 1, 1);
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['Font'] = Enum['Font']['Fantasy'];
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['TextSize'] = 18;
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['TextTransparency'] = 0.4;
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['Text'] = suggestion;
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['TextXAlignment'] = Enum['TextXAlignment']['Center'];
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['Activated']:Connect(function()
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["ChatBar%0"]['Text'] = suggestion;
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["ChatBar%0"]:CaptureFocus();
			end);
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"]['Parent'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["SuggestionFrame%0"]['Frame'];
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["corner%0"] = Instance.new("UICorner");
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["corner%0"]['CornerRadius'] = UDim.new(0, 4);
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["corner%0"]['Parent'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["button%0"];
		end
	end);
end
TABLE_TableIndirection["TABLE_TableIndirection%0"]["GrimoireTab%0"]:Toggle({Title="Spells AutoTextBox",Desc="Dont Toggle this more than once or there will be multiple boxes xoxo",Default=false,Callback=function(state)
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["CustomSpellsEnabled%0"] = state;
	if state then
		EnableSpellAutofill();
	end
end});
