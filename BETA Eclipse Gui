	if _G['SelectedCharacter'] then
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["outfitDropdown%0"]:Refresh(_G['SelectedCharacter'].Outfits);
	end
end});
TABLE_TableIndirection["TABLE_TableIndirection%0"]["CharacterTab%0"]:Button({Title="Change Character",Callback=function()
	if (_G['SelectedCharacter'] and _G['SelectedOutfit']) then
		ChangeCharacter(_G['SelectedCharacter'].Name, _G.SelectedOutfit);
	else
		print("Please select a character and an outfit.");
	end
end});
