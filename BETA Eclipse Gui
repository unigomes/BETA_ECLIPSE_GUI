TABLE_TableIndirection["TABLE_TableIndirection%0"]["TVL2Effects%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["MiscTab%0"]:Dropdown({Title="TVL2 World Effects",Multi=false,Values={"Remove TVL1 Skybox","Add TVL2 SkyBox","TVL2 Lighting","TVL2 Atmosphere","TVL2 SunRays","TVL2 ColorCorrection","TVL2 DepthOfField"},Callback=function(selectedEffect)
	if (selectedEffect == "Remove TVL1 Skybox") then
		removeTVL1Skybox();
	elseif (selectedEffect == "Add TVL2 SkyBox") then
		addTVL2Skybox();
	elseif (selectedEffect == "TVL2 Lighting") then
		applyTVL2Lighting();
	elseif (selectedEffect == "TVL2 Atmosphere") then
		applyTVL2Atmosphere();
	elseif (selectedEffect == "TVL2 SunRays") then
		applyTVL2SunRays();
	elseif (selectedEffect == "TVL2 ColorCorrection") then
		applyTVL2ColorCorrection();
	elseif (selectedEffect ~= "TVL2 DepthOfField") then
	else
		applyTVL2DepthOfField();
	end
end});
