end
TABLE_TableIndirection["TABLE_TableIndirection%0"]["HopeExtraSpells%0"] = {{Name="Repulse",DefaultKey="B",Remote="HopeRepulse"},{Name="Scream",DefaultKey="Y",Remote="HopeScream"}};
local function addHopeExtraSpells(tab, spells)
	tab:Section({Title="Hope's Special Abilities"});
	for _, spell in ipairs(spells) do
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["toggleActive%0"] = true;
		tab:Input({Title=(spell['Name'] .. " (" .. spell['DefaultKey'] .. ")"),Placeholder=spell['DefaultKey'],Callback=function(input)
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["newKey%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["NumberKeyMap%0"][input] or input:upper();
			if Enum['KeyCode'][TABLE_TableIndirection["TABLE_TableIndirection%0"]["newKey%0"]] then
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["ActiveKeybinds%0"][spell['DefaultKey']] = nil;
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["ActiveKeybinds%0"][TABLE_TableIndirection["TABLE_TableIndirection%0"]["newKey%0"]] = function()
					if TABLE_TableIndirection["TABLE_TableIndirection%0"]["toggleActive%0"] then
						castHopeSpell(spell.Remote);
					end
				end;
				spell['DefaultKey'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["newKey%0"];
			end
		end});
		tab:Toggle({Title=("Activate " .. spell['Name']),Default=true,Callback=function(Value)
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["toggleActive%0"] = Value;
		end});
	end
end
addHopeExtraSpells(TABLE_TableIndirection["TABLE_TableIndirection%0"]["KeybindsTab%0"], TABLE_TableIndirection["TABLE_TableIndirection%0"]["HopeExtraSpells%0"]);
