		if TABLE_TableIndirection["TABLE_TableIndirection%0"]["data%0"] then
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["WindUI%0"]:Notify({Title="File Loaded",Content=("Loaded data: " .. TABLE_TableIndirection["TABLE_TableIndirection%0"]["HttpService%0"]:JSONEncode(TABLE_TableIndirection["TABLE_TableIndirection%0"]["data%0"])),Duration=5});
			if TABLE_TableIndirection["TABLE_TableIndirection%0"]["data%0"]['Transparent'] then
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["Window%0"]:ToggleTransparency(TABLE_TableIndirection["TABLE_TableIndirection%0"]["data%0"].Transparent);
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["ToggleTransparency%0"]:SetValue(TABLE_TableIndirection["TABLE_TableIndirection%0"]["data%0"].Transparent);
			end
			if TABLE_TableIndirection["TABLE_TableIndirection%0"]["data%0"]['Theme'] then
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["WindUI%0"]:SetTheme(TABLE_TableIndirection["TABLE_TableIndirection%0"]["data%0"].Theme);
			end
		end
	end
end});
TABLE_TableIndirection["TABLE_TableIndirection%0"]["ConfigTab%0"]:Button({Title="Overwrite File",Callback=function()
	if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["fileNameInput%0"] == "") then
	else
		SaveFile(TABLE_TableIndirection["TABLE_TableIndirection%0"]["fileNameInput%0"], {Transparent=TABLE_TableIndirection["TABLE_TableIndirection%0"]["WindUI%0"]:GetTransparency(),Theme=TABLE_TableIndirection["TABLE_TableIndirection%0"]["WindUI%0"]:GetCurrentTheme()});
	end
end});
TABLE_TableIndirection["TABLE_TableIndirection%0"]["ConfigTab%0"]:Button({Title="Refresh List",Callback=function()
	filesDropdown:Refresh(ListFiles());
end});
TABLE_TableIndirection["TABLE_TableIndirection%0"]["currentThemeName%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["WindUI%0"]:GetCurrentTheme();
TABLE_TableIndirection["TABLE_TableIndirection%0"]["themes%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["WindUI%0"]:GetThemes();
TABLE_TableIndirection["TABLE_TableIndirection%0"]["ThemeAccent%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["themes%0"][TABLE_TableIndirection["TABLE_TableIndirection%0"]["currentThemeName%0"]]['Accent'];
TABLE_TableIndirection["TABLE_TableIndirection%0"]["ThemeOutline%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["themes%0"][TABLE_TableIndirection["TABLE_TableIndirection%0"]["currentThemeName%0"]]['Outline'];
TABLE_TableIndirection["TABLE_TableIndirection%0"]["ThemeText%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["themes%0"][TABLE_TableIndirection["TABLE_TableIndirection%0"]["currentThemeName%0"]]['Text'];
TABLE_TableIndirection["TABLE_TableIndirection%0"]["ThemePlaceholderText%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["themes%0"][TABLE_TableIndirection["TABLE_TableIndirection%0"]["currentThemeName%0"]]['PlaceholderText'];
function updateTheme()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["WindUI%0"]:AddTheme({Name=TABLE_TableIndirection["TABLE_TableIndirection%0"]["currentThemeName%0"],Accent=TABLE_TableIndirection["TABLE_TableIndirection%0"]["ThemeAccent%0"],Outline=TABLE_TableIndirection["TABLE_TableIndirection%0"]["ThemeOutline%0"],Text=TABLE_TableIndirection["TABLE_TableIndirection%0"]["ThemeText%0"],PlaceholderText=TABLE_TableIndirection["TABLE_TableIndirection%0"]["ThemePlaceholderText%0"]});
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["WindUI%0"]:SetTheme(TABLE_TableIndirection["TABLE_TableIndirection%0"]["currentThemeName%0"]);
end
TABLE_TableIndirection["TABLE_TableIndirection%0"]["CreateInput%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["ThemeTab%0"]:Input({Title="Theme Name",Value=TABLE_TableIndirection["TABLE_TableIndirection%0"]["currentThemeName%0"],Callback=function(name)
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["currentThemeName%0"] = name;
end});
