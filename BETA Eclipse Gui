local function getAllowedCharacterNames()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["names%0"] = {};
	for _, char in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["availableCharacters%0"]) do
		for _, allowed in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["allowedCharacters%0"]) do
			if (char['Name'] == allowed) then
				table.insert(TABLE_TableIndirection["TABLE_TableIndirection%0"]["names%0"], char.Name);
				break;
			end
		end
	end
	return TABLE_TableIndirection["TABLE_TableIndirection%0"]["names%0"];
end
local function getFirstOutfit(characterName)
	for _, char in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["availableCharacters%0"]) do
		if (char['Name'] == characterName) then
			return char['Outfits'][1];
		end
	end
	return nil;
end
TABLE_TableIndirection["TABLE_TableIndirection%0"]["CharacterTab%0"]:Paragraph({Title="Attention",Desc="Characters selected must be free, if you are mixed with your avatar and no outfit gui appears that means one character is taken, free it then try again"});
TABLE_TableIndirection["TABLE_TableIndirection%0"]["CharacterTab%0"]:Dropdown({Title="Powers from:",Values=getAllowedCharacterNames(),Callback=function(value)
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPower%0"] = value;
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedCharacter%0"] = value;
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedOutfit%0"] = getFirstOutfit(value);
end});
TABLE_TableIndirection["TABLE_TableIndirection%0"]["CharacterTab%0"]:Dropdown({Title="Looks from (Outfit):",Values=(function()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["names%0"] = {};
	for _, char in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["availableCharacters%0"]) do
		table.insert(TABLE_TableIndirection["TABLE_TableIndirection%0"]["names%0"], char.Name);
	end
	return TABLE_TableIndirection["TABLE_TableIndirection%0"]["names%0"];
end)(),Callback=function(value)
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedCharacter%0"] = value;
	for _, char in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["availableCharacters%0"]) do
		if (char['Name'] ~= value) then
		else
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedOutfit%0"] = char['Outfits'][1];
			break;
		end
	end
