local function IsCharTaken(charName)
	for _, player in pairs(game:GetService("Players"):GetPlayers()) do
		if (player['Character'] and (player['Character']['Name'] == charName)) then
			return true;
		end
	end
	return false;
end
TABLE_TableIndirection["TABLE_TableIndirection%0"]["MiscTab%0"]:Section({Title="TVL2"});
TABLE_TableIndirection["TABLE_TableIndirection%0"]["Lighting%0"] = game:GetService("Lighting");
local function removeTVL1Skybox()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["effectsToRemove%0"] = {"Blur","Peaceful Morning Sky","ColorCorrection","SunRays","Bloom"};
	while true do
		for _, effectName in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["effectsToRemove%0"]) do
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["effect%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Lighting%0"]:FindFirstChild(effectName);
			if TABLE_TableIndirection["TABLE_TableIndirection%0"]["effect%0"] then
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["effect%0"]:Destroy();
				print(effectName .. " removed.");
			end
		end
		wait(1);
	end
end
local function addTVL2Skybox()
	local function configureSky(sky)
		sky['SkyboxBk'] = "http://www.roblox.com/asset/?id=150335574";
		sky['SkyboxDn'] = "http://www.roblox.com/asset/?id=150335585";
		sky['SkyboxFt'] = "http://www.roblox.com/asset/?id=150335628";
		sky['SkyboxLf'] = "http://www.roblox.com/asset/?id=150335620";
		sky['SkyboxRt'] = "http://www.roblox.com/asset/?id=150335610";
		sky['SkyboxUp'] = "http://www.roblox.com/asset/?id=150335642";
		sky['StarCount'] = 1334;
		sky['MoonTextureId'] = "rbxassetid://1345054856";
		sky['MoonAngularSize'] = 60;
		sky['SunTextureId'] = "rbxassetid://1345009717";
		sky['SunAngularSize'] = 12;
	end
	while true do
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["sky%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Lighting%0"]:FindFirstChildOfClass("Sky");
		if not TABLE_TableIndirection["TABLE_TableIndirection%0"]["sky%0"] then
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["sky%0"] = Instance.new("Sky");
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["sky%0"]['Parent'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Lighting%0"];
			print("SkyBox created.");
		end
		configureSky(TABLE_TableIndirection["TABLE_TableIndirection%0"]["sky%0"]);
		print("SkyBox configured.");
		wait(10);
	end
end
local function applyTVL2Lighting()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["settings%0"] = {Ambient=Color3.fromRGB(93, 93, 93),Brightness=1,ColorShift_Bottom=Color3.fromRGB(0, 0, 0),ColorShift_Top=Color3.fromRGB(216, 180, 116),EnvironmentDiffuseScale=0,GlobalShadows=true,OutdoorAmbient=Color3.fromRGB(118, 146, 189),ShadowSoftness=0.2,GeographicLatitude=23.5};
	while true do
		for setting, value in pairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["settings%0"]) do
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["Lighting%0"][setting] = value;
		end
		wait(0.1);
	end
end
local function applyTVL2Atmosphere()
	while true do
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Lighting%0"]:FindFirstChild("FoggyAtmosphere");
		if not TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"] then
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"] = Instance.new("Atmosphere");
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"]['Name'] = "FoggyAtmosphere";
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"]['Parent'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Lighting%0"];
		end
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"]['Density'] = 0.4;
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"]['Offset'] = 0;
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"]['Color'] = Color3.fromRGB(16, 18, 34);
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"]['Decay'] = Color3.fromRGB(78, 88, 92);
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"]['Glare'] = 0.16;
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["atmosphere%0"]['Haze'] = 0;
		wait(10);
	end
end
