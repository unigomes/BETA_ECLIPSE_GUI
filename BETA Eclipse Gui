	TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPlayer%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%7"]:FindFirstChild(TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPlayerName%0"]);
