	TABLE_TableIndirection["TABLE_TableIndirection%0"]["multiSpamActive%0"] = state;
	if state then
		spawn(function()
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["targets%0"] = {};
			for target in string.gmatch(TABLE_TableIndirection["TABLE_TableIndirection%0"]["multiTargetInput%0"], "([^,]+)") do
				table.insert(TABLE_TableIndirection["TABLE_TableIndirection%0"]["targets%0"], target:lower());
			end
			while TABLE_TableIndirection["TABLE_TableIndirection%0"]["multiSpamActive%0"] do
				for _, targetName in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["targets%0"]) do
					for _, p in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%11"]:GetPlayers()) do
						if ((p['Name']:lower() == targetName) and p['Character']) then
							for _, spell in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedMultiSpamSpells%0"]) do
								TABLE_TableIndirection["TABLE_TableIndirection%0"]["args%0"] = {[1]={Incant=spell,Target=p['Character']}};
								TABLE_TableIndirection["TABLE_TableIndirection%0"]["ReplicatedStorage%9"]:WaitForChild("RemoteEvents"):WaitForChild("WitchSpell"):FireServer(unpack(TABLE_TableIndirection["TABLE_TableIndirection%0"]["args%0"]));
							end
						end
					end
				end
				wait(0);
			end
		end);
	end
end});
