			TABLE_TableIndirection["TABLE_TableIndirection%0"]["depthOfField%0"] = Instance.new("DepthOfFieldEffect");
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["depthOfField%0"]['Parent'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Lighting%0"];
		end
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["depthOfField%0"]['FarIntensity'] = 0.05;
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["depthOfField%0"]['FocusDistance'] = 0.05;
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["depthOfField%0"]['InFocusRadius'] = 30;
