local function updatePlayerList()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["playerNames%0"] = {};
	for _, player in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%10"]:GetPlayers()) do
		table.insert(TABLE_TableIndirection["TABLE_TableIndirection%0"]["playerNames%0"], player.Name);
	end
	return TABLE_TableIndirection["TABLE_TableIndirection%0"]["playerNames%0"];
end
