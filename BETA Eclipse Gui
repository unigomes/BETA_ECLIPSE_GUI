TABLE_TableIndirection["TABLE_TableIndirection%0"]["SpamTab%0"]:Toggle({Title="Spy Target",Default=false,Callback=function(state)
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["localPlayer%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%10"]['LocalPlayer'];
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["myChar%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["localPlayer%0"]['Character'];
	if not TABLE_TableIndirection["TABLE_TableIndirection%0"]["myChar%0"] then
		return;
	end
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["myHumanoid%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["myChar%0"]:WaitForChild("Humanoid");
	if state then
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"] = nil;
		if TABLE_TableIndirection["TABLE_TableIndirection%0"]["spamModeIsCharacter%0"] then
			for _, p in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%10"]:GetPlayers()) do
				if p:FindFirstChild("CharacterConfiguration") then
					TABLE_TableIndirection["TABLE_TableIndirection%0"]["charName%0"] = p['CharacterConfiguration']['CharacterName']['Value']:lower();
					if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["charName%0"] ~= TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetInput%0"]) then
					else
						TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"] = p['Character'];
						break;
					end
				end
			end
		else
			for _, p in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%10"]:GetPlayers()) do
				if (p['Name']:lower() == TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetInput%0"]) then
					TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"] = p['Character'];
					break;
				end
			end
		end
		if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"] and TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"]:FindFirstChild("Humanoid")) then
			workspace['CurrentCamera']['CameraSubject'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetCharacter%0"]:FindFirstChild("Humanoid");
		end
	else
		workspace['CurrentCamera']['CameraSubject'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["myHumanoid%0"];
	end
end});
