			TABLE_TableIndirection["TABLE_TableIndirection%0"]["ActiveKeybinds%0"][TABLE_TableIndirection["TABLE_TableIndirection%0"]["keyName%0"]]();
		end
	end
end);
local function addSpellSection(tab, category)
	tab:Section({Title=category['Title']});
	for _, spell in ipairs(category.Spells) do
		tab:Input({Title=(spell['Name'] .. " (" .. spell['DefaultKey'] .. ")"),Placeholder=spell['DefaultKey'],Callback=function(input)
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["newKey%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["NumberKeyMap%0"][input] or input:upper();
			if Enum['KeyCode'][TABLE_TableIndirection["TABLE_TableIndirection%0"]["newKey%0"]] then
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["ActiveKeybinds%0"][TABLE_TableIndirection["TABLE_TableIndirection%0"]["DefaultKeybinds%0"][spell['Name']]] = nil;
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["ActiveKeybinds%0"][TABLE_TableIndirection["TABLE_TableIndirection%0"]["newKey%0"]] = function()
					castSpell(spell);
				end;
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["DefaultKeybinds%0"][spell['Name']] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["newKey%0"];
			end
		end});
	end
end
for _, category in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["SpellCategories%0"]) do
	addSpellSection(TABLE_TableIndirection["TABLE_TableIndirection%0"]["KeybindsTab%0"], category);
end
