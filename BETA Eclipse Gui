TABLE_TableIndirection["TABLE_TableIndirection%0"]["harvestButton%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["HarvestTab%0"]:Button({Title="ðŸŒ¿ Harvest",Callback=function()
