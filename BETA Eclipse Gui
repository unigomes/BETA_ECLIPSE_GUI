		TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPlayer%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Players%7"]:FindFirstChild(TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPlayerName%0"]);
		if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPlayer%0"] and TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPlayer%0"]['Character']) then
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetRoot%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPlayer%0"]['Character']:FindFirstChild("HumanoidRootPart");
