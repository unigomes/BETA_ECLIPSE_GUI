local function SaveFile(fileName, data)
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["filePath%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["folderPath%0"] .. "/" .. fileName .. ".json";
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["jsonData%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["HttpService%0"]:JSONEncode(data);
	writefile(TABLE_TableIndirection["TABLE_TableIndirection%0"]["filePath%0"], TABLE_TableIndirection["TABLE_TableIndirection%0"]["jsonData%0"]);
end
local function LoadFile(fileName)
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["filePath%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["folderPath%0"] .. "/" .. fileName .. ".json";
	if isfile(TABLE_TableIndirection["TABLE_TableIndirection%0"]["filePath%0"]) then
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["jsonData%0"] = readfile(TABLE_TableIndirection["TABLE_TableIndirection%0"]["filePath%0"]);
		return TABLE_TableIndirection["TABLE_TableIndirection%0"]["HttpService%0"]:JSONDecode(TABLE_TableIndirection["TABLE_TableIndirection%0"]["jsonData%0"]);
	end
end
local function ListFiles()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["files%0"] = {};
	for _, file in ipairs(listfiles(TABLE_TableIndirection["TABLE_TableIndirection%0"]["folderPath%0"])) do
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["fileName%0"] = file:match("([^/]+)%.json$");
		if TABLE_TableIndirection["TABLE_TableIndirection%0"]["fileName%0"] then
			table.insert(TABLE_TableIndirection["TABLE_TableIndirection%0"]["files%0"], TABLE_TableIndirection["TABLE_TableIndirection%0"]["fileName%0"]);
		end
	end
	return TABLE_TableIndirection["TABLE_TableIndirection%0"]["files%0"];
end
