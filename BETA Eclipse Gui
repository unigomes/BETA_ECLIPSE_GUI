local function lockDoor()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["args%0"] = {[1]={Locked=true,Target=workspace:WaitForChild("Doors"):WaitForChild(TABLE_TableIndirection["TABLE_TableIndirection%0"]["doorName%0"]):WaitForChild("radius"),Operation="ManageDoor"}};
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["ReplicatedStorage%1"]:WaitForChild("Events"):WaitForChild("DoorInteract"):FireServer(unpack(TABLE_TableIndirection["TABLE_TableIndirection%0"]["args%0"]));
end
local function startLockLoop()
	while TABLE_TableIndirection["TABLE_TableIndirection%0"]["doorLocked%0"] do
		lockDoor();
		wait(0.05);
	end
end
