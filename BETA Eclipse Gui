			TABLE_TableIndirection["TABLE_TableIndirection%0"]["Character%2"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Player%4"]['Character'];
			TABLE_TableIndirection["TABLE_TableIndirection%0"]["myRoot%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["Character%2"] and TABLE_TableIndirection["TABLE_TableIndirection%0"]["Character%2"]:FindFirstChild("HumanoidRootPart");
			if (TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetRoot%0"] and TABLE_TableIndirection["TABLE_TableIndirection%0"]["myRoot%0"]) then
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["OldCFrame%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["myRoot%0"]['CFrame'];
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["myRoot%0"]['CFrame'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["targetRoot%0"]['CFrame'];
				wait(1);
				TABLE_TableIndirection["TABLE_TableIndirection%0"]["pentagram%0"] = workspace:FindFirstChild("SolarisPentagram");
				if TABLE_TableIndirection["TABLE_TableIndirection%0"]["pentagram%0"] then
					TABLE_TableIndirection["TABLE_TableIndirection%0"]["portal%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["pentagram%0"]:FindFirstChild("SolarisIn");
					if TABLE_TableIndirection["TABLE_TableIndirection%0"]["portal%0"] then
						TABLE_TableIndirection["TABLE_TableIndirection%0"]["myRoot%0"]['CFrame'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["portal%0"]['CFrame'];
						wait(0.5);
						TABLE_TableIndirection["TABLE_TableIndirection%0"]["HarvestEvent%0"]:FireServer(TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPlayer%0"]);
						wait(1.5);
						TABLE_TableIndirection["TABLE_TableIndirection%0"]["myRoot%0"]['CFrame'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["OldCFrame%0"];
						print("Harvest executed on: " .. TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPlayerName%0"]);
					else
						print("Portal not found in SolarisPentagram");
					end
				else
					print("SolarisPentagram not found");
				end
			else
				print("Target or player root part missing");
			end
		else
			print("Player not found or missing character parts: " .. TABLE_TableIndirection["TABLE_TableIndirection%0"]["selectedPlayerName%0"]);
		end
	else
		print("Please enter a username.");
	end
end});
TABLE_TableIndirection["TABLE_TableIndirection%0"]["refreshButton%0"]:Callback();
local function ChangeCharacter(newCharName, newOutfit)
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["oldPosition%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%1"]['Character'] and TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%1"]['Character']['PrimaryPart'] and TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%1"]['Character']['PrimaryPart']['CFrame'];
	local success, characterList = pcall(function()
		return TABLE_TableIndirection["TABLE_TableIndirection%0"]["ReplicatedStorage%5"]:WaitForChild("RemoteEvents"):WaitForChild("GetCharacterList"):InvokeServer();
	end);
	if (not success or not characterList or (characterList[newCharName] ~= "None")) then
		warn("Character unavailable: " .. newCharName);
		return;
	end
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["ReplicatedStorage%5"]:WaitForChild("Events"):WaitForChild("PlayerDeath"):FireServer({Option="respawn",Approved=true});
	repeat
		task.wait();
	until TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%1"]['PlayerGui']:FindFirstChild("Menu") 
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%1"]['PlayerGui']['Menu']:Destroy();
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["ReplicatedStorage%5"]:WaitForChild("Events"):WaitForChild("SpawnAsCharacter"):FireServer({CharName=newCharName,Outfit=newOutfit});
	repeat
		task.wait();
	until TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%1"]['Character'] and TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%1"]['Character']:FindFirstChild("HumanoidRootPart") 
	if TABLE_TableIndirection["TABLE_TableIndirection%0"]["oldPosition%0"] then
		TABLE_TableIndirection["TABLE_TableIndirection%0"]["LocalPlayer%1"]['Character']:SetPrimaryPartCFrame(TABLE_TableIndirection["TABLE_TableIndirection%0"]["oldPosition%0"]);
	end
	print("Character changed to: " .. newCharName);
end
_G['SelectedCharacter'] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["availableCharacters%0"][1];
_G['SelectedOutfit'] = _G['SelectedCharacter']['Outfits'][1];
local function updateCharacterList()
	TABLE_TableIndirection["TABLE_TableIndirection%0"]["charNames%0"] = {};
	for _, char in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["availableCharacters%0"]) do
		table.insert(TABLE_TableIndirection["TABLE_TableIndirection%0"]["charNames%0"], char.Name);
	end
	return TABLE_TableIndirection["TABLE_TableIndirection%0"]["charNames%0"];
end
TABLE_TableIndirection["TABLE_TableIndirection%0"]["CharacterTab%0"]:Section({Title="Respawn as character"});
TABLE_TableIndirection["TABLE_TableIndirection%0"]["characterDropdown%0"] = TABLE_TableIndirection["TABLE_TableIndirection%0"]["CharacterTab%0"]:Dropdown({Title="Select Character",Values=updateCharacterList(),Value=(updateCharacterList()[1] or ""),Callback=function(selectedName)
	for _, char in ipairs(TABLE_TableIndirection["TABLE_TableIndirection%0"]["availableCharacters%0"]) do
		if (char['Name'] ~= selectedName) then
		else
			_G['SelectedCharacter'] = char;
			_G['SelectedOutfit'] = char['Outfits'][1];
			break;
		end
	end
	outfitDropdown:Refresh(_G['SelectedCharacter'].Outfits);
end});
